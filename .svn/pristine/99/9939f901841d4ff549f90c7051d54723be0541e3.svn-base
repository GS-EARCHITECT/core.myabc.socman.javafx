package application.login.DTO;

import java.io.Serializable;

public class Login_DTO implements Serializable {
	     private String userid;
	     private String password;
	     private String recordNo;
	     private String roleNo;
	          
	     public Login_DTO()
	     {}
	 	    
	     public Login_DTO(String b_userId,	String b_password,	String b_recordno, String b_roleNo) 
	    		    {
	    			try {
	    				this.userid = b_userId;
	    				this.password = b_password;
	    				this.recordNo = b_recordno;	    				
	    				this.roleNo= b_roleNo;
	    				
	    			} catch (Exception e) {
	    				// TODO Auto-generated catch block
	    				System.out.println("Here");
	    				e.printStackTrace();
	    			}
	    		    }

		public String getRoleNo() {
			return roleNo;
		}

		public void setRoleNo(String roleNo) {
			this.roleNo = roleNo;
		}

		public String getUserid() {
			return userid;
		}

		public void setUserid(String userid) {
			this.userid = userid;
		}

		public String getPassword() {
			return password;
		}

		public void setPassword(String password) {
			this.password = password;
		}

		public String getRecordNo() {
			return recordNo;
		}

		public void setRecordNo(String recordNo) {
			this.recordNo = recordNo;
		}	     
}
