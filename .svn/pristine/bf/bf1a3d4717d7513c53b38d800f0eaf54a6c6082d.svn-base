package application.login.Helper;

import java.io.Serializable;

public class Login_Bean implements Serializable {
	     private String userid;
	     private String password;
	     private String recordNo;	     
	          
	     public Login_Bean()
	     {}
	 	    
	     public Login_Bean(String b_userId,	String b_password,	String b_recordno) 
	    		    {
	    			try {
	    				this.userid = b_userId;
	    				this.password = b_password;
	    				this.recordNo = b_recordno;	    				
	    				
	    			} catch (Exception e) {
	    				// TODO Auto-generated catch block
	    				System.out.println("Here");
	    				e.printStackTrace();
	    			}
	    		    }

		public String getUserid() {
			return userid;
		}

		public void setUserid(String userid) {
			this.userid = userid;
		}

		public String getPassword() {
			return password;
		}

		public void setPassword(String password) {
			this.password = password;
		}

		public String getRecordNo() {
			return recordNo;
		}

		public void setRecordNo(String recordNo) {
			this.recordNo = recordNo;
		}	     
}
